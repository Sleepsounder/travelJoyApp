<p id = "alert"><%= alert %></p>
<%= form_tag sessions_path do |form| %>
    <div class = "field">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div class = "field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class = "actions">
      <%= submit_tag "Log In" %>
    </div>
<% end %>
<p>or</p>
<div><%= link_to "Sign Up", new_user_path %></div>




